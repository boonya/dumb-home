{
  "name": "dumb-home",
  "version": "1.0.0",
  "description": "SmartHome Web Application which is actually quite dumb",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boonya/dumb-home.git"
  },
  "keywords": [
    "SmartHome",
    "home",
    "IoT",
    "WebCAM",
    "video",
    "node",
    "meteor",
    "reactjs",
    "control",
    "raspberry",
    "pi",
    "raspi"
  ],
